{
    "product_id": {
        "type": "crazyselect",
        "template": "crazyselect",
        "label": "Sản phẩm",
        "call": "get_product_select_options",
        "params":[{"id":":defval"}],
        "@select-type": "search",
        "@search-route": "admin.products.select-options"
    },
    "type": {
        "type": "crazyselect",
        "template": "crazyselect",
        "label": "Hành động",
        "required": "true",
        "data":{
            "import": "Nhập kho", "export": "Xuất kho"
        }
    },
    "total": {
        "type": "number",
        "label": "Số lượng",
        "placeholder": "Nhập số lượng",
        "min": 1,
        "step": 1
    },
    "note": {
        "type": "textarea",
        "label": "Ghi chú",
        "placeholder": "Nhập ghi chú"
    }
}