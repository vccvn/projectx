{
    "url": {
        "type": "text",
        "label": "Đường dẫn (URL)",
        "placeholder": "Ví dụ: https://news.zing.vn/abc-post123.html",
        "required": "true"
    },
    "frame_id": {
        "type": "crazyselect",
        "label": "Nguồn",
        "template": "crazyselect",
        "call": "get_crawler_frame_options",
        "required": "true"
    },
    "crawl_resources": {
        "type": "checkbox",
        "label": "Crawl Tài nguyên",
        "template": "switch",
        "@options": {
            "form_group_class":"row",
            "label_class": "col-6 col-md-4 col-lg-3 col-xl-2 col-form-label",
            "wrapper_class": "col-6 col-md-8 col-lg-9 col-xl-10"
        }
    },
    "category_id": {
        "type": "crazyselect",
        "label": "Danh mục",
        "template": "crazyselect",
        "call": "get_product_category_options"
        
    },
    "author_id": {
        "type": "crazyselect",
        "label": "Người đăng bài",
        "template": "crazyselect",
        "@select-type": "search",
        "@search-route": "admin.users.select-option",
        "call": "get_staff_options",
        "params": [
            {
                "id": ":defval"
            }
        ]
    }
}